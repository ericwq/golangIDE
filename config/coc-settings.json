{
  "go.goplsArgs": ["-remote", "auto", "-logfile", "/tmp/gopls.log"],
  "go.goplsPath": "/go/bin/gopls",
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true,
    "experimentalWorkspaceModule": true
  },
  "markdownlint.config": {
	  "default":true,
	  "line_length": false
  }
}
